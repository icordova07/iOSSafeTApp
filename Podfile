  pod 'AWSDynamoDB'
  # Pods for SafeT
  
  target 'project493Tests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'project493UITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
